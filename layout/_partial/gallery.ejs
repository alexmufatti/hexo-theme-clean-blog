
<div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
    <div class="row">
        <% if (item.photos.length){ %>
            <% item.photos.forEach(function(photo, idx){ %>
                <div class="col-lg-3 col-md-4 col-sm-4 col-xs-6 cssbox">
                    <a id="image<%- idx %>" href="#image<%- idx %>"><img class="cssbox_thumb" src="<%- photo %>" />
                        <span class="cssbox_full"><img src="<%- photo %>" /></span>
                    </a>
                    <a class="cssbox_close" href="#void"></a>
                    <a class="cssbox_prev" href="#image<%- idx - 1 %>">&lt;</a>
                    <a class="cssbox_next" href="#image<%- idx + 1%>">&gt;</a>
                </div>
            <% }); %>
        <% } %>
    </div>
</div>
